<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.deepanshu.quiz"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="18"
        android:targetSdkVersion="26" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <meta-data
        android:name="android.support.VERSION"
        android:value="26.0.0-alpha1" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.deepanshu.quiz.Login"
            android:label="Login" />
        <activity android:name="com.deepanshu.quiz.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.deepanshu.quiz.Register"
            android:label="Register"
            android:parentActivityName="com.deepanshu.quiz.Login" />
        <activity
            android:name="com.deepanshu.quiz.QBDetails"
            android:parentActivityName="com.deepanshu.quiz.MainActivity" />
        <activity android:name="com.deepanshu.quiz.Add_Questions" />
        <activity
            android:name="com.deepanshu.quiz.Result_User"
            android:label="Result"
            android:parentActivityName="com.deepanshu.quiz.UserActivity" />
        <activity
            android:name="com.deepanshu.quiz.TestDetails"
            android:parentActivityName="com.deepanshu.quiz.MainActivity" />
        <activity
            android:name="com.deepanshu.quiz.DisplayQB"
            android:parentActivityName="com.deepanshu.quiz.MainActivity" />
        <activity
            android:name="com.deepanshu.quiz.DisplayTest"
            android:parentActivityName="com.deepanshu.quiz.MainActivity" />
        <activity android:name="com.deepanshu.quiz.UserActivity" />
        <activity
            android:name="com.deepanshu.quiz.StartTest"
            android:label="Test Details"
            android:parentActivityName="com.deepanshu.quiz.UserActivity" />
        <activity android:name="com.deepanshu.quiz.Question" />
        <activity
            android:name="com.deepanshu.quiz.TestResult"
            android:label="Test" />
        <activity
            android:name="com.deepanshu.quiz.AdminResults"
            android:label="Results"
            android:parentActivityName="com.deepanshu.quiz.UserActivity" />
    </application>

</manifest>